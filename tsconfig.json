{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "wwwroot/js",
    "rootDir": "ThreeJS"
  },
  "include": ["ThreeJS/**/*"]
}